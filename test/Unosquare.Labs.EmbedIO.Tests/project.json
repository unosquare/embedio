{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [ "html" ]
  },
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-3",
    "NUnit": "3.7.1",
    "Unosquare.Labs.EmbedIO": { "target": "project" }
  },

  "testRunner": "nunit",

  "frameworks": {
    "netcoreapp1.1": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": "1.1.2"
      }
    },
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": "1.1.2"
      }
    },
    "net462": {
      "frameworkAssemblies": {
        "System.Net.Http": "",
        "System.Threading.Tasks": ""
      },
      "buildOptions": { "define": [ "NET47" ] }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Net.Http": "",
        "System.Threading.Tasks": ""
      }
    }
  },

  "runtimes": {
    "win8-x64": {},
    "ubuntu.14.04-x64": {},
    "osx.10.11-x64": {}
  }
}