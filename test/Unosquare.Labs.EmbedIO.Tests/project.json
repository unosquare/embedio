{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [ "html" ]
  },
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-1",
    "Newtonsoft.Json": "9.0.1",
    "Unosquare.Labs.EmbedIO": "1.0.0-*",
    "NUnit": "3.5.0"
  },

  "testRunner": "nunit",

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        },
        "System.Net.Http": "4.1.0"
      }
    },
    "net452": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      },
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1-*"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      },
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1-*"
      }
    }
  }
}